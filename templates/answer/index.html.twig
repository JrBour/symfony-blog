{% extends 'base.html.twig' %}
{% block title %}Réponses{% endblock %}
{% block body %}
    <div class="example-wrapper">
        {% if answers %}
            <ul>
                {% for answer in answers %}
                    <li>{{answer.content}}</li>
                {% endfor %}
            </ul>
        {% else %}
            <h2>Aucune réponse n'a était créée</h2>
        {% endif %}
    </div>
{% endblock %}
